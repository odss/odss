    "lint": "eslint src/*",
    "format": "prettier src/ --write",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@odss/common": "1.0.0",
    "@odss/shell": "1.0.0"
  },
  "devDependencies": {
    "@stool/micropack": "0.3.0",
    "@types/chai": "4.2.21",
    "@types/mocha": "8.2.3",
    "@types/node": "16.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "eslint": "7.30.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "2.1.1",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.1",
    "mocha": "9.0.2",
    "prettier": "2.3.2",
    "sinon": "11.1.1",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  },
  "files": [
    "README.md",
    "dist/*.*",
    "dist/types/*.d.ts"
  ]
}
