{
  "name": "@odss/shell.core",
  "version": "1.0.0",
  "description": "ODSS Shell Core",
  "license": "MIT",
  "author": "smialy",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types2": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "micropack",
    "build:dev": "npm run build -- --watch",
