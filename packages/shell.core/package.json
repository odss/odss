{
  "name": "@odss/shell.core",
  "version": "1.0.0",
  "description": "ODSS Shell Core",
  "license": "MIT",
  "author": "smialy",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "browser": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "micropack",
    "build:dev2": "micropack --dev",
    "lint": "eslint src/*",
    "format": "prettier src/ tests/ --write",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --require ts-node/register tests/*.test.ts",
    "test:karma": "karma start --single-run",
    "test:cov": "nyc npm run test:mocha",
    "test:cov:html": "nyc --reporter=html npm run test:mocha",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@odss/common": "1.0.0",
    "@odss/shell": "1.0.0"
  },
  "devDependencies": {
    "@stool/micropack": "0.4.0",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.15.1",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "chai": "^4.3.7",
    "eslint": "8.36.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "2.8.4",
    "sinon": "^15.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "files": [
    "README.md",
    "dist/*.*",
    "dist/types/*.d.ts"
  ]
}
