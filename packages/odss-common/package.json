{
  "name": "@odss/common",
  "version": "1.0.0",
  "description": "ODSS Common",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "microbundle build --no-compress --no-sourcemap --target=node -f es,cjs",
    "lint": "eslint src/*",
    "format": "prettier src/ tests --write",
    "prepare": "npm run build"
  },
  "files": [
    "README.md",
    "dist/*.js",
    "dist/*.mjs",
    "dist/types/*.d.ts"
  ],
  "devDependencies": {
    "@types/qunit": "^2.11.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
