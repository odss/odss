{
    "name": "@odss/cdi",
    "version": "1.0.0",
    "description": "Odss Cdi Core",
    "license": "MIT",
    "source": "src/index.ts",
    "main": "dist/cdi.js",
    "module": "dist/cdi.mjs",
    "scripts": {
        "build": "micropack",
        "build:dev": "npm run build -- --watch",
        "test": "mocha --require ts-node/register tests/*.ts",
        "karma": "karma start --single-run",
        "lint": "eslint src/*",
        "format": "prettier src/ tests --write",
        "prepare": "npm run build"
    },
    "exports": {
        ".": {
            "import": "./dist/cdi.mjs",
            "require": "./dist/cdi.js"
        }
    },
    "dependencies": {
        "@stool/logging": "3.1.0",
        "@odss/common": "1.0.0"
    },
    "devDependencies": {
        "@stool/micropack": "0.3.0",
        "@types/chai": "4.2.21",
        "@types/mocha": "8.2.3",
        "@types/node": "16.3.1",
        "@types/reflect-metadata": "0.1.0",
        "@typescript-eslint/eslint-plugin": "4.28.3",
        "eslint": "7.30.0",
        "karma": "6.3.4",
        "karma-chrome-launcher": "3.1.0",
        "karma-firefox-launcher": "2.1.1",
        "karma-mocha": "2.0.1",
        "karma-typescript": "5.5.1",
        "mocha": "9.0.2",
        "prettier": "2.3.2",
        "reflect-metadata": "0.1.13",
        "sinon": "11.1.1",
        "ts-node": "10.1.0",
        "typescript": "4.3.5"
    },
    "files": [
        "dist/*.js",
        "dist/*.mjs",
        "dist/types/*.d.ts",
        "README.md"
    ]
}
