{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "max-line-length": [100],
    "quotemark": [true, "single"],
    "member-access": false,
    "max-classes-per-file": [false],
    "ordered-imports": [false],
    "object-literal-sort-keys": false,
    "callable-types": false,
    "arrow-parens": false,
    "no-empty": false,
    "ban-types": false
  },
  "rulesDirectory": []
}
