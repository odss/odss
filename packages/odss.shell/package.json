{
  "name": "@odss/shell",
  "version": "1.0.0",
  "description": "ODSS Shell",
  "license": "MIT",
  "author": "smialy",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/shell.js",
  "module": "dist/shell.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/shell.mjs",
      "require": "./dist/shell.js"
    }
  },
  "scripts": {
    "build": "microbundle build --no-compress --no-sourcemap --target=node -f es,cjs",
    "build:dev": "npm run build:core -- --watch",
    "test": "mocha --require ts-node/register tests/*.ts tests/**/*.ts",
    "karma": "karma start --single-run",
    "lint": "eslint src/*",
    "format": "prettier src/ --write",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@odss/common": "1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.1",
    "microbundle": "^0.13.0",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "ts-node": "^9.1.1"
  }
}
