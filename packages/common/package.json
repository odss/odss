{
  "name": "@odss/common",
  "version": "1.0.0",
  "description": "ODSS Common",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/common.js",
  "module": "dist/common.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/common.mjs",
      "import": "./dist/common.mjs",
      "require": "./dist/common.js"
    }
  },
  "scripts": {
    "build": "micropack --include reflect-metadata",
    "build:dev": "npm run build -- --dev",
    "lint": "eslint src/*",
    "format": "prettier src/ tests/ --write",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --require ts-node/register tests/*.test.ts",
    "test:karma": "karma start --single-run",
    "test:cov": "nyc npm run test:mocha",
    "test:cov:html": "nyc --reporter=html npm run test:mocha",
    "prepare": "npm run build"
  },
  "dependencies": {
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@stool/micropack": "0.4.0",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.15.1",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "chai": "^4.3.7",
    "eslint": "8.36.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.3",
    "mocha": "10.2.0",
    "prettier": "2.8.4",
    "sinon": "15.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "files": [
    "README.md",
    "dist/*.*",
    "dist/**/*.d.ts"
  ]
}
