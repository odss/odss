{
  "name": "@odss/config",
  "version": "1.0.0",
  "description": "Odss Core",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "micropack",
    "build:dev2": "micropack --dev",
    "lint": "eslint src/*",
    "format": "prettier src/ --write",
    "prepare": "npm run build",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --require ts-node/register tests/*.spec.ts",
    "test:karma": "karma start --single-run",
    "test:cov": "nyc npm run test:mocha",
    "test:cov:html": "nyc --reporter=html npm run test:mocha"
  },
  "exports": {
    ".": {
      "browser": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@odss/common": "1.0.0",
    "@odss/shell": "1.0.0"
  },
  "devDependencies": {
    "@stool/micropack": "0.4.0",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "chai": "^4.3.7",
    "eslint": "8.13.0",
    "karma": "6.3.17",
    "karma-chrome-launcher": "3.1.1",
    "karma-mocha": "2.0.1",
    "karma-typescript": "5.5.3",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "2.6.2",
    "sinon": "^15.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.6.3"
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/types/*.d.ts",
    "README.md"
  ]
}
